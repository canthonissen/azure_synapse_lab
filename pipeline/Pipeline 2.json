{
	"name": "Pipeline 2",
	"properties": {
		"activities": [
			{
				"name": "InsertEntityToStorageTable",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": {
						"value": "@concat('https://',pipeline().parameters.storage_account,'.table.core.windows.net/',pipeline().parameters.table_name, '()?',pipeline().parameters.sas)",
						"type": "Expression"
					},
					"connectVia": {
						"referenceName": "AutoResolveIntegrationRuntime",
						"type": "IntegrationRuntimeReference"
					},
					"method": "POST",
					"headers": {
						"Content-Type": "application/json",
						"Content-Length": {
							"value": "@length(string(pipeline().parameters.data_json))",
							"type": "Expression"
						},
						"x-ms-date": {
							"value": "@utcnow()",
							"type": "Expression"
						},
						"Accept": "application/json;odata=nometadata"
					},
					"body": {
						"value": "@string(pipeline().parameters.data_json)",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"storage_account": {
				"type": "string",
				"defaultValue": "bucketeer"
			},
			"table_name": {
				"type": "string",
				"defaultValue": "PipelineStatus2"
			},
			"data_json": {
				"type": "object",
				"defaultValue": {
					"Address": "Mountain Dew",
					"Age": 23,
					"AmountDue": 200.23,
					"PartitionKey": "mypartitionkey",
					"RowKey": "myrowkey"
				}
			},
			"sas": {
				"type": "string",
				"defaultValue": "sv=2022-11-02&ss=t&srt=sco&sp=rwdlacu&se=2025-01-23T01%3A52%3A02Z&st=2024-01-22T17%3A52%3A02Z&spr=https&sig=Z2UDZ8JkRliog1UV6G8byWZhOLYABz73Td4VqyipFos="
			}
		},
		"annotations": []
	}
}